<h1>Login To Bread Trail.</h1>

<% if @error %>
    <p style="color: red;"><%= @error %></p>
<% end %>
<br>
<%= link_to "Home", posts_path %>




<%= form_tag('/login') do %>
    <%= label_tag("Username: ") %>
    <%= text_field_tag(:username) %>
    <%= label_tag("Password: ") %>
    <%= password_field_tag(:password) %>
    <%= submit_tag("Login") %>
<% end %>
<br>
<br>
<br>


